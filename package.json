{
  "name": "package-install-cmd",
  "version": "1.0.0",
  "description": "Lists the dependency and dev dependency in the project and generates commands to install them using npm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "start": "tsc && node dist/cli.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "testWithCoverage": "nyc -r lcov -e .ts -x \"*.test.ts\" mocha -r ts-node/register tests/**/*.test.ts && nyc report",
    "deploy-local": "tsc && (npm install -g) && package-install-cmd"
  },
  "bin": {
    "package-install-cmd": "./dist/cli.js",
    "pic": "./dist/cli.js"
  },
  "author": "Chirag",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiragrupani/cgfp.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.5.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {}
}
